{
    "name": "mahone3297/doctrine-base-bundle",
    "description": "It provides some common doctrine base features.",
    "keywords": [
        "doctrine"
    ],
    "type": "library",
    "require": {
        "php": "~7.0",
        "doctrine/doctrine-bundle": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "overtrue/phplint": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mahone3297",
            "email": "329730566@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mahone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mahone\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit -c phpunit.xml.dist"
        ],
        "lint": [
            "./vendor/bin/phplint"
        ]
    }
}
